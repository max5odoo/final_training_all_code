<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
      
        <template id="portal_Existing_Order" name="portal Existing Order" customize_show="True" inherit_id="portal.portal_my_home" priority="21">
            <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
                <t t-call="portal.portal_docs_entry">
                    <t t-set="title">Check Existing Order</t>
                    <t t-set="url" t-value="'/my/check_order'"/>
                    <t t-set="placeholder_count" t-value="'order_count'"/>
        </t>
            </xpath>
        </template>
    </data>
</odoo>